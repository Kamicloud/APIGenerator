{
    "name": "kamicloud/stub-api",
    "description": "API Generator",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "kamicloud",
            "email": "cy602939074@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kamicloud\\StubApi\\": "src/main/php/lib/"
        }
    },
    "require": {
        "nikic/php-parser": "^4.2"
    },
    "require-dev": {
        "ext-json": "*",
        "laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*|6.*|7.*|8.*",
        "binarytorch/larecipe": "^1.0|^2.0",
        "phpstan/phpstan": "^0.11.16"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kamicloud\\StubApi\\GeneratorServiceProvider"
            ]
        }
    },
    "suggest": {
        "binarytorch/larecipe": "Docs support",
        "kamicloud/laravel-stub-api-larecipe": "Some optimize based on larecipe",
        "laravel/telescope": ""
    }
}
